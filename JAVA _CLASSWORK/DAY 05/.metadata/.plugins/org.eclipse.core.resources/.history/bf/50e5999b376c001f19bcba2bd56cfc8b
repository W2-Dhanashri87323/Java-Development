package com.dhanashri;

public class Program {

	public static void main(String[] args) {
	   
	 Duck d = new Duck();	
     // Flyable f = d; // upcasting
      
      Flyable f = d; // polymorphic declaration
      
     f.fly(); 
     f.eat(); 
      
      
 //    d.fly(); // not ok or not good practice 
 //    d.eat(); // not ok
    
      d.name(); 
  
 
		
	}

}
 